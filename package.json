{
    "name": "cn",
    "displayName": "CodeNow",
    "description": "Empower your ServiceNow development with zero configuration.",
    "version": "0.9",
    "publisher": "j27q6tasx6z32tftojfekap44umwkigmng2bf3gmj6aapra6zzya",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Syspeople/snsb"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets.json"
            },
            {
                "language": "html",
                "path": "./html_snippets.json"
            }
        ],
        "configuration": [
            {
                "title": "",
                "properties": {
                    "cn.uploadOnSave": {
                        "type": "boolean",
                        "default": true,
                        "description": "Automatically upload record on save",
                        "scope": "window"
                    },
                    "cn.addOnOpen": {
                        "type": "boolean",
                        "default": true,
                        "description": "On open automatically add record to workspace record if outdated",
                        "scope": "window"
                    }
                }
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "group": "cn",
                    "command": "cn.updateRecord",
                    "when": "resourceScheme == file"
                },
                {
                    "group": "cn",
                    "command": "cn.saveRecord",
                    "when": "resourceScheme == file"
                },
                {
                    "group": "cn",
                    "command": "cn.openInPlatformRecord",
                    "when": "resourceScheme == file"
                },
                {
                    "group": "cn",
                    "command": "cn.openInPlatformList",
                    "when": "resourceScheme == file"
                },
                {
                    "group": "cn",
                    "command": "cn.deleteRecord",
                    "when": "resourceScheme == file"
                }
            ],
            "commandPalette": [
                {
                    "command": "cn.saveRecord",
                    "when": "false"
                },
                {
                    "command": "cn.updateRecord",
                    "when": "false"
                },
                {
                    "command": "cn.openInPlatformRecord",
                    "when": "false"
                },
                {
                    "command": "cn.openInPlatformList",
                    "when": "false"
                },
                {
                    "command": "cn.deleteRecord",
                    "when": "false"
                }
            ]
        },
        "commands": [
            {
                "command": "cn.openInPlatformRecord",
                "title": "Open Record in Platform",
                "category": "CodeNow"
            },
            {
                "command": "cn.openInPlatformList",
                "title": "Open List in Platform",
                "category": "CodeNow"
            },
            {
                "command": "cn.setUpdateset",
                "title": "Change Update Set",
                "category": "CodeNow"
            },
            {
                "command": "cn.connect",
                "title": "Connect to ServiceNow",
                "category": "CodeNow"
            },
            {
                "command": "cn.clearWorkSpaceState",
                "title": "Clear Instance",
                "category": "CodeNow"
            },
            {
                "command": "cn.rebuildCache",
                "title": "Refresh Records",
                "category": "CodeNow"
            },
            {
                "command": "cn.addRecord",
                "title": "Add Record to Workspace",
                "category": "CodeNow"
            },
            {
                "command": "cn.saveRecord",
                "title": "Save Record",
                "category": "CodeNow"
            },
            {
                "command": "cn.updateRecord",
                "title": "Update Record",
                "category": "CodeNow"
            },
            {
                "command": "cn.createUpdateSet",
                "title": "Create Updateset",
                "category": "CodeNow"
            },
            {
                "command": "cn.createRecord",
                "title": "Create Record",
                "category": "CodeNow"
            },
            {
                "command": "cn.deleteRecord",
                "title": "Delete Record",
                "category": "CodeNow"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.0.0",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/opn": "^5.1.0",
        "axios": "^0.18.0",
        "mixpanel": "^0.10.2",
        "opn": "^5.4.0"
    }
}